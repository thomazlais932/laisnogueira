def bubble_sort(arr):
    n = len(arr)
    # Percorre todos os elementos do array
    for i in range(n):
        # Últimos i elementos já estão na posição correta
        for j in range(0, n-i-1):
            # Percorre o array de 0 até n-i-1
            # Troca se o elemento encontrado for maior que o próximo elemento
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]

# Teste do algoritmo
arr = [64, 34, 25, 12, 22, 11, 90]
print("Array original:", arr)
bubble_sort(arr)
print("Array ordenado:", arr)


———————-

# Frase
frase = "o céu é azul"

# Lista de vogais
vogais = "aeiouAEIOU"

# Itera sobre cada caractere na frase e imprime se for uma vogal
for char in frase:
    if char in vogais:
        print(char, end=" ")  # Usando end=" " para imprimir na mesma linha

# Output: o é u é a u

                 
